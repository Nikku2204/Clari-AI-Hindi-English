      "createdByID": 1628484,
      "folderID": null,
      "code": "export default async function main(args) {\n  const { inputVars } = args;\n  \n  // Get the file URL from last_event.payload.file\n  const fileUrl = inputVars.last_event?.payload?.file;\n  \n  // Check if a file was successfully uploaded\n  if (fileUrl) {\n    console.log(\"File uploaded successfully: \" + fileUrl);\n    \n    // Return success response with file URL\n    return {\n      outputVars: {\n        upload_success: true,\n        uploaded_file_url: fileUrl\n      },\n      next: {\n        path: 'success'\n      }\n    };\n  } else {\n    // Handle upload failure\n    console.log(\"File upload failed or was cancelled\");\n    \n    return {\n      outputVars: {\n        upload_success: false,\n        error_message: \"File upload failed or was cancelled\"\n      },\n      next: {\n        path: 'error'\n      }\n    };\n  }\n}\n}",
